$catalog AtClass local
class AtClass

    private name
    private parentList
    private methodList
    private annotationList
    
    public sub create.object(nameValue)
        name = nameValue
        parentList = object("LinkedList")
        methodList = object("LinkedList")
        annotationList = object("LinkedList")
    end
    
    public sub addParent(parent)
        parentList->add(parent)
    end
    
    public sub addMethod(method)
        methodList->add(method)
    end
    
    public sub addAnnotation(annotation)
        annotationList->add(annotation)
    end
    
    **
    * getters/setters
    **
    
    get getName()
        return name
    end
    
    set setName(value)
        name = value
    end
    
    get getParentList()
        return parentList
    end
    
    set setParentList(list)
        parentList = list
    end
    
    get getMethodList()
        return methodList
    end
    
    set setMethodList(list)
        methodList = list
    end
    
    get getAnnotationList()
        return annotationList
    end
    
    set setAnnotationList(list)
        annotationList = list
    end

end
