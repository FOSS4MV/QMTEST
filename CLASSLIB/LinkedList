$catalog LinkedList local
class LinkedList

    private head, tail, size
    
    public sub create.object()
        head = ''
        tail = ''
        size = 0
    end
    
    public sub add(obj)
        node = object("LinkedListNode", obj)
        if (size = 0) then
            head = node
            tail = node
            size = 1
        end else
            tail->setNext(node)
            size += 1
        end
    end

end
