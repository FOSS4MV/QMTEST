$catalog AtTest local
class AtTest

    private method
    
    get getMethod()
        return method
    end
    
    set setMethod(value)
        if (objinfo(value, 0)) then
            if (objinfo(value, 1) = "ATMETHOD") then
                method = value
                return
            end
            throw "NoAtMethodAnno", "Test classes must be preprocessed to add @Method annotations!"
        end
        throw "NotAnObject", "AtTest->setMethod(value): value must be an AtMethod object, got <" : value : "> instead!"
    end
    
    public sub run()
        

end
